#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    
    int n;cin>>n;
    vector<int>arr;
    int v;
    while(n--){
        cin>>v;
        arr.push_back(v);
    }
    
    int q; cin>>q;
    while(q--)
    {
        cin>>v;
        int index = lower_bound(arr.begin(),arr.end(), v) - arr.begin();
        
        if( arr[index] == v){
            cout<<"Yes "<<index+1<<endl;
        }
        else{
            cout<<"No "<<index+1<<endl;
        }
    }
       
    return 0;
}
