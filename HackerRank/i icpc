#include <bits/stdc++.h>
using namespace std;
bool ismultiple(const vector<int>&v,int num){
    int cnt=0;
    for(int x:v){
        if(x==num)
        cnt++;
        if(cnt>1)
        return true;
    }
    return false;
}

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	 int n;
	 cin>>n;
	 vector<int>v(n);
	 for(int i=0;i<n;i++)
	 cin>>v[i];
	 vector<int>v1=v;
	 sort(v1.begin(),v1.end());
	 int i;
	 for(i=0;i<n;i++){
	     if(v[i]==v1[i]&& !ismultiple(v1,v1[i]))
	     break;
	     
	     
	 }
	 if(i==n)
	 cout<<"Case "<<t<<"Humanity is doomed!"<<endl;
	
	 cout<<"Case:"<<t<<i+1<<endl;
	 

}
}
