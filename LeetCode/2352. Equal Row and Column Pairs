/***** Brute force solution TC- O(N^3) ***************/
class Solution {
public:
    int equalPairs(vector<vector<int>>& grid) {
        int sz = grid.size();
       vector< vector<int> >cgrid(sz, vector<int>(sz) );

        for(int j=0; j<sz; j++)
        {
            for(int i=0; i<sz; i++){
                cgrid[j][i] = (grid[i][j]);
            }
        }

        vector<int>row;
        int cnt=0, k=0;

        for(int i=0; i<sz; i++){
            for(int j=0; j<sz; j++){
                row.push_back(grid[i][j]);
            }

            for(int ip=0; ip<sz; ip++){
                int elm=0, p=0;
                for( int jp=0; jp<sz; jp++ ){

                    if(row[jp] == cgrid[ip][jp]){
                        elm++;
                        
                        if(elm==sz){
                            cnt++;
                        }
                    }
                    else{
                        break;
                    }
                }
            }
            row.clear();   
        }
        return cnt;
    }
};
