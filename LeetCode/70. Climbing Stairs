class Solution {

static const int mx = 50;
int vis[mx];
int dp[mx];
vector<int>arr={1,2};
int cnt=0;

int dfs(int sum)
{
    if(sum==0)return 1;
    if(dp[sum]!=0)return dp[sum];
    
    int res=0;

    for(int i=0; i<arr.size(); i++){
        if(sum-arr[i] >= 0){
           res += dfs(sum-arr[i]);
        }
    }

   vis[sum]=1;
   dp[sum]=res;

    return res;
}

public:
    int climbStairs(int n) {

       return dfs(n);
    }
};
