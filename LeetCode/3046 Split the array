class Solution {
public:
    bool isPossibleToSplit(vector<int>& nums) {

        int sz = nums.size();
        vector<int>freq(101,0);

        for(int i=0; i<sz; i++){
                    
                freq[nums[i]]++;
                
                if( freq[nums[i]] > 2 )return false;
         
        }
        
        return true;
    }
};
